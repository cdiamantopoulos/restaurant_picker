<%= render 'layouts/logged_out_header' %>



<div align = "center">

<% flash.each do |key, value| %>
  <div class="alert-temp alert fade in alert-<%= key %>"><%= value %></div>
<% end %>

<h1>Log In</h1>
<%=form_tag login_path do%>
<%=label_tag :email %>
</br>
<%=text_field_tag :email %>
</br>
<%=label_tag :password %>
</br>
<%=password_field_tag :password %>
</br>
<%=submit_tag "Log in", class: "btn btn-large btn-primary" %>
<%end%>

</div>